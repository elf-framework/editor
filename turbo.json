{
  "pipeline": {
    "watch": {
      "dependsOn": ["^watch"],
      "outputs": ["dist/**", ".next/**"]
    },    
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["watch"],
      "cache": false
    },
    "blank:watch": {
      "dependsOn": ["^blank:watch"],
      "outputs": ["dist/**", ".next/**"]          
    },    
    "blank:dev": {
      "dependsOn": ["blank:watch"],
      "cache": false
    },
    "design:watch": {
      "dependsOn": [
        "^design:watch"
      ],
      "outputs": ["dist/**", ".next/**"]      
    },
    "ui:watch": {
      "dependsOn": [
        "^ui:watch"
      ],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },    
    "design:dev": {
      "dependsOn": ["design:watch"],
      "cache": false
    },
    "ui:dev": {
      "dependsOn": ["ui:watch"],
      "cache": false
    },
    "editor:watch": {
      "dependsOn": [
        "^editor:watch"
      ],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },        
    "editor:dev": {
      "dependsOn": ["editor:watch"]
    },    
    "ui-react:watch": {
      "dependsOn": [
        "^ui-react:watch"
      ],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },        
    "ui-react:dev": {
      "dependsOn": ["ui-react:watch"]
    },        
    "sapa:release": {
      "dependsOn": ["^sapa:release"],
      "outputs": ["dist/**", ".next/**"]
    }                                            

  }
}
