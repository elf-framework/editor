{
  "pipeline": {
    "watch": {
      "dependsOn": ["^watch"],
      "outputs": ["dist/**", ".next/**"]
    },    
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["watch"],
      "cache": false
    },
    "ui:watch": {
      "dependsOn": [
        "^ui:watch"
      ],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },    
    "ui:dev": {
      "dependsOn": ["ui:watch"],
      "cache": false
    },
    "sapa:watch": {
      "dependsOn": [
        "^sapa:watch"
      ],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },    
    "sapa:dev": {
      "dependsOn": ["sapa:watch"],
      "cache": false
    },    
    "ssr:watch": {
      "dependsOn": [
        "^ssr:watch"
      ],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },    
    "ssr:dev": {
      "dependsOn": ["ssr:watch"],
      "cache": false
    },    
    "editor:watch": {
      "dependsOn": [
        "^editor:watch"
      ],
      "outputs": ["dist/**", ".next/**"],
      "cache": false
    },        
    "editor:dev": {
      "dependsOn": ["editor:watch"]
    }
  }
}
