{
  "name": "@elf-framework/design-system",
  "version": "0.0.1",
  "description": "Fantastic Design System ",
  "main": "./dist/design-system.umd.js",
  "module": "./dist/design-system.es.js",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./dist/design-system.es.js",
      "require": "./dist/design-system.umd.js"
    }
  },
  "dependencies": {
    "open-color": "^1.9.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "cssnano": "^5.0.13",
    "husky": "^7.0.4",
    "postcss": "^8.4.4",
    "prettier": "2.6.2",
    "release-it": "^14.6.2",
    "sass": "^1.44.0",
    "vite": "^2.9.1",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^1.0.5",
    "vite-plugin-eslint": "^1.5.2",
    "vite-plugin-replace": "^0.1.1",
    "vitest": "0.0.107"
  },
  "scripts": {
    "vite:dev": "vite --config=vite.config.js",
    "vite:dist": "vite build --config=vite.dist.config.js",
    "//watch": "vite build --config=vite.dist.config.js --watch",
    "design:watch": "vite build --config=vite.dist.config.js --watch",
    "ui:watch": "vite build --config=vite.dist.config.js --watch",
    "release": "release-it *",
    "build": "npm run vite:dist",
    "//prepare": "husky install",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easylogic/editor.git"
  },
  "keywords": [
    "CSS",
    "CODE",
    "Editor",
    "Path",
    "SVG"
  ],
  "author": "easylogic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/easylogic/editor/issues"
  },
  "homepage": "https://editor.easylogic.studio/"
}
