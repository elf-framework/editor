{
  "pipeline": {
    "watch": {
      "dependsOn": [
        "editor#watch",
        "^watch"
      ],
      "outputs": ["dist/**", ".next/**"]
    },    
    "build": {
      "dependsOn": [
        "editor#build",
        "^build"
      ],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["watch"],
      "cache": false
    }
  }
}
