{
  "pipeline": {
    "watch": {
      "dependsOn": [
        "sapa#watch",
        "color#watch",
        "icon#watch",
        "^watch"
      ],
      "outputs": ["dist/**", ".next/**"]
    },    
    "blank:watch": {
      "dependsOn": [
        "sapa#blank:watch",
        "color#blank:watch",
        "icon#blank:watch",
        "^blank:watch"
      ],
      "outputs": ["dist/**", ".next/**"]
    },        
    "build": {
      "dependsOn": [
        "sapa#build",
        "color#build",
        "icon#build",
        "^build"
      ],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
