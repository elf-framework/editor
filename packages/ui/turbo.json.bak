{
  "pipeline": {
    "ui:watch": {
      "dependsOn": [
        "sapa#ui:watch",
        "design-system#ui:watch",
        "^ui:watch"
      ],
      "outputs": ["dist/**", ".next/**"]
    },    
    "blank:watch": {
      "dependsOn": [
        "^blank:watch"
      ],
      "outputs": ["dist/**", ".next/**"]
    },        
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
